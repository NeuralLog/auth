{
  "name": "neurallog-auth",
  "version": "0.1.0",
  "description": "Authentication and authorization service for NeuralLog",
  "main": "dist/index.js",
  "scripts": {
    "generate-openapi": "node scripts/generate-openapi.js",
    "generate-api-types": "node scripts/generate-auth-types.js",
    "prebuild": "npm run generate-openapi && npm run generate-api-types",
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "docker:build": "docker build -t neurallog-auth .",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down"
  },
  "keywords": [
    "neurallog",
    "auth",
    "openfga",
    "authorization"
  ],
  "author": "NeuralLog Team",
  "license": "MIT",
  "dependencies": {
    "@neurallog/client-sdk": "^0.1.10",
    "@openfga/sdk": "^0.8.0",
    "@types/ioredis": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "helmet": "^5.0.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.0",
    "pg": "^8.7.0",
    "uuid": "^11.1.0",
    "winston": "^3.7.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/js-yaml": "^4.0.5",
    "js-yaml": "^4.1.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.0",
    "@types/pg": "^8.6.0",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "eslint": "^8.13.0",
    "jest": "^27.5.0",
    "ts-jest": "^27.1.0",
    "ts-node-dev": "^1.1.0",
    "typescript": "^4.6.0"
  }
}
